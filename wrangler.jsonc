{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "oop-io",
  "compatibility_date": "2025-08-03",
  "main": "./src/server/index.ts",
  "assets": {
    "directory": "./dist/client",
    "binding": "ASSETS"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "ROOM",
        "class_name": "Room",
        "script_name": "oop-io"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_classes": ["Room"]
    }
  ]
}